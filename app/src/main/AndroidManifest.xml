<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sulijin.androidfinalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

    <activity android:name=".StartActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="zhentingmai.androidfinalproject.AutomobileActivity"
            android:label="@string/title_activity_automobile" />
        <activity
            android:name=".ActivityTrackingActivity"
            android:label="@string/title_activity_tracking"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar" />

        <activity
            android:name="tianjunjin.androidfinalproject.FoodActivity"
            android:label="@string/title_activity_food" />


    </application>

</manifest>