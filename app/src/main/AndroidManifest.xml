        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
    <activity
            android:name=".StartActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tianjunjin.androidfinalproject.NutritionActivity"
            android:label="@string/title_activity_nutrition" />
        <activity
            android:name="zhentingmai.androidfinalproject.AutomobileActivity"
            android:label="@string/title_activity_automobile" />


        <activity
            android:name=".ActivityTrackingActivity"
            android:label="@string/title_activity_tracking"
